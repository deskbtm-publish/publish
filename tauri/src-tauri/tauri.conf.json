{
  "$schema": "../../node_modules/@tauri-apps/cli/schema.json",
  "productName": "Publish",
  "version": "0.0.1",
  "identifier": "com.deskbtm.publish",
  "build": {
    "frontendDist": "../../packages/desktop/dist",
    "devUrl": "https://localhost:3000",
    "beforeDevCommand": "yarn workspace @publish/desktop dev",
    "beforeBuildCommand": "yarn workspace @publish/desktop build"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "decorations": false,
        "transparent": false,
        "title": "Publish",
        "label": "main",
        "width": 1160,
        "height": 800,
        "resizable": true,
        "fullscreen": false,
        "center": true,
        "dragDropEnabled": false
      }
    ],
    "security": {},
    "macOSPrivateApi": true
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [],
    "externalBin": [],
    "copyright": "",
    "category": "DeveloperTool",
    "shortDescription": "",
    "longDescription": "",
    "macOS": {
      "frameworks": [],
      "minimumSystemVersion": "",
      "exceptionDomain": "",
      "signingIdentity": null,
      "entitlements": null
    },
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": "",
      "nsis": {
        "languages": [
          "English",
          "SimpChinese"
        ]
      }
    }
  },
  "plugins": {
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEJFMzA3MDg5REFFQTgzMjEKUldRaGcrcmFpWEF3dm5qL0JlZUxVNmtreVZaNmNEaFd3eHJNNzRnYTNQRUhDc1U2WExUdERVNE8K"
    }
  }
}
