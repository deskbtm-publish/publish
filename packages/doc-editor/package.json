{
  "name": "@publish/doc-editor",
  "version": "0.0.1-alpha.1",
  "description": "Publish document editor",
  "author": "Han <han@deskbtm.com>",
  "main": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./*": {
      "default": "./src/*"
    }
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc",
    "prepack": "npm run build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "devDependencies": {
    "@mantine/core": "7.4.0",
    "@mantine/hooks": "7.4.0",
    "@tabler/icons-react": "2.46.0",
    "typescript": "5.3.3"
  },
  "peerDependencies": {
    "@mantine/core": "7.4.0",
    "@mantine/hooks": "7.4.0",
    "@tabler/icons-react": "2.46.0"
  },
  "dependencies": {
    "@tiptap/core": "2.2.1",
    "@tiptap/extension-code-block-lowlight": "2.2.1",
    "@tiptap/extension-link": "2.2.1",
    "@tiptap/extension-placeholder": "2.2.1",
    "@tiptap/extension-task-item": "2.2.1",
    "@tiptap/extension-task-list": "2.2.1",
    "@tiptap/extension-underline": "2.2.1",
    "@tiptap/pm": "2.2.1",
    "@tiptap/react": "2.2.1",
    "@tiptap/starter-kit": "2.2.1",
    "@tiptap/suggestion": "2.2.1",
    "lowlight": "3.1.0",
    "tiptap-markdown": "0.8.9"
  }
}
