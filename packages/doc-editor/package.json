{
  "name": "@publish/doc-editor",
  "version": "0.0.1-alpha.1",
  "description": "Publish document editor",
  "author": "Han <han@deskbtm.com>",
  "main": "./src/index.ts",
  "private": true,
  "exports": {
    ".": "./src/index.ts",
    "./preset/*.ts": {
      "default": "./src/preset/*.ts"
    }
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc",
    "prepack": "npm run build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "devDependencies": {
    "@mantine/core": "7.5.3",
    "@mantine/hooks": "7.5.3",
    "@tabler/icons-react": "2.46.0",
    "typescript": "5.3.3"
  },
  "peerDependencies": {
    "@mantine/core": "7.5.3",
    "@mantine/hooks": "7.5.3",
    "@tabler/icons-react": "2.46.0"
  },
  "dependencies": {
    "@sereneinserenade/tiptap-search-and-replace": "^0.1.1",
    "@tiptap/core": "2.2.3",
    "@tiptap/extension-code-block-lowlight": "2.2.3",
    "@tiptap/extension-color": "2.2.3",
    "@tiptap/extension-link": "2.2.3",
    "@tiptap/extension-placeholder": "2.2.3",
    "@tiptap/extension-table": "^2.2.3",
    "@tiptap/extension-table-cell": "^2.2.3",
    "@tiptap/extension-table-header": "^2.2.3",
    "@tiptap/extension-table-row": "^2.2.3",
    "@tiptap/extension-task-item": "2.2.3",
    "@tiptap/extension-task-list": "2.2.3",
    "@tiptap/extension-text-style": "2.2.3",
    "@tiptap/extension-underline": "2.2.3",
    "@tiptap/pm": "2.2.3",
    "@tiptap/react": "2.2.3",
    "@tiptap/starter-kit": "2.2.3",
    "@tiptap/suggestion": "2.2.3",
    "lowlight": "3.1.0",
    "tiptap-markdown": "0.8.9"
  }
}
