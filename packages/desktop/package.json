{
  "name": "@publish/desktop",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "cross-env VITE_CJS_IGNORE_WARNING=true vite",
    "dev:remote": "cross-env VITE_CJS_IGNORE_WARNING=true ENABLE_REMOTE_DEBUG=true vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --fix --report-unused-disable-directives --max-warnings 0",
