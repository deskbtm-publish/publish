{
  "name": "@publish/desktop",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "lint": "eslint . --ext ts,tsx --fix --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "build": "cross-env NODE_ENV=production tsx ../../node_modules/webpack/bin/webpack.js -c publish-pack.config.ts",
    "dev": "cross-env NODE_ENV=development tsx ../../node_modules/webpack/bin/webpack.js serve -c publish-pack.config.ts",
    "dev:remote": "cross-env ENABLE_REMOTE_DEBUG=true vite"
  },
  "dependencies": {
    "@deskbtm/gadgets": "0.0.1-alpha.10",
    "@mantine/core": "7.6.1",
    "@mantine/dates": "7.6.1",
    "@mantine/dropzone": "7.6.1",
    "@mantine/form": "7.6.1",
    "@mantine/hooks": "7.6.1",
    "@mantine/notifications": "7.6.1",
    "@mantine/nprogress": "7.6.1",
    "@mantine/spotlight": "7.6.1",
    "@publish/doc-editor": "workspace:^",
    "@publish/logger": "workspace:^",
    "@publishjs/x-api": "workspace:^",
    "@tauri-apps/api": "2.0.0-beta.4",
    "@tauri-apps/plugin-autostart": "^2.0.0-beta.2",
    "@tauri-apps/plugin-shell": "2.0.0-beta.2",
    "react-router": "6.22.2",
    "react-router-dom": "6.22.3"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "8.56.0",
    "tsx": "4.7.1"
  }
}
