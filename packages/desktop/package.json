{
  "name": "@publish/desktop",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "cross-env VITE_CJS_IGNORE_WARNING=true vite",
    "dev:remote": "cross-env VITE_CJS_IGNORE_WARNING=true ENABLE_REMOTE_DEBUG=true vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --fix --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "a": "cross-env NODE_ENV=production tsx ../../node_modules/webpack/bin/webpack.js -c publish-pack.config.ts",
    "b": "cross-env NODE_ENV=development tsx ../../node_modules/webpack/bin/webpack.js serve -c publish-pack.config.ts"
  },
  "dependencies": {
    "@deskbtm/gadgets": "0.0.1-alpha.8",
    "@mantine/core": "7.6.1",
    "@mantine/dates": "7.6.1",
    "@mantine/dropzone": "7.6.1",
    "@mantine/form": "7.6.1",
    "@mantine/hooks": "7.6.1",
    "@mantine/notifications": "7.6.1",
    "@mantine/nprogress": "7.6.1",
    "@mantine/spotlight": "7.6.1",
    "@publish/doc-editor": "workspace:^",
    "@publish/logger": "workspace:^",
    "react-router": "6.21.1",
    "react-router-dom": "6.21.1"
  },
  "devDependencies": {
    "@types/react-dom": "18.2.18",
    "cross-env": "^7.0.3",
    "eslint": "8.56.0",
    "vite-plugin-istanbul": "5.0.0"
  }
}
