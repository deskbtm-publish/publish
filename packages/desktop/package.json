  "dependencies": {
    "@mantine/core": "7.5.3",
    "@mantine/dates": "7.5.3",
    "@mantine/dropzone": "7.5.3",
    "@mantine/form": "7.5.3",
    "@mantine/hooks": "7.5.3",
    "@mantine/notifications": "7.5.3",
    "@mantine/nprogress": "7.5.3",
    "@mantine/spotlight": "7.5.3",
    "@mantine/vanilla-extract": "7.5.3",
    "@publish/doc-editor": "workspace:^",
    "@publish/logger": "workspace:^",
    "@tiptap/extension-underline": "^2.2.3",
    "react-router": "6.21.1",
    "react-router-dom": "6.21.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "8.56.0",
    "vite": "5.0.12",
    "vite-plugin-istanbul": "^5.0.0"
  }
